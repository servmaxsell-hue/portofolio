{
    "version": 2,
    "env": {
        "DATABASE_URL": "@database_url"
    },
    "build": {
        "env": {
            "DATABASE_URL": "@database_url"
        }
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts"
        }
    ]
}